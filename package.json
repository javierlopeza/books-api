{
  "name": "books-admin",
  "version": "1.0.0",
  "description": "The coolest books admin app!",
  "author": "Javier Lopez <javierlopez@uc.cl>",
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "db:setup": "export NODE_ENV=development && npx sequelize db:create; export NODE_ENV=test && npx sequelize db:create",
    "db:reset": "export NODE_ENV=development && npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "dev": "export NODE_ENV=development && nodemon --exec babel-node ./api/index.js",
    "start": "node ./build/index.js",
    "build": "rm -rf ./build && babel -d ./build ./api -s",
    "heroku-postbuild": "yarn build && sequelize db:migrate",
    "test": "export NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate && nyc --require @babel/register mocha ./api/tests/ --timeout 20000 --exit",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc yarn test && yarn generate-lcov && yarn coveralls-coverage && yarn codeclimate-coverage"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-loader": "^8.0.6",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "camelcase-keys": "^6.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chrono-node": "^1.3.11",
    "codeclimate-test-reporter": "^0.5.1",
    "cookie-session": "^1.3.3",
    "coveralls": "^3.0.6",
    "dotenv": "^8.1.0",
    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "nyc": "^14.1.1",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pkgcloud": "^2.1.1",
    "sequelize": "^5.17.1",
    "sequelize-cli": "^5.5.0",
    "webpack": "^4.39.3"
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.1"
  }
}
